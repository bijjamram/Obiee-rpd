<rules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.xebialabs.com/xl-deploy/xl-rules" >

    <rule name="obiee.Catalog.CREATE_OR_UPDATE" scope="deployed">
        <conditions>
            <type>obiee.Catalog</type>
            <operation>CREATE</operation>
        </conditions>
        <steps>
            <os-script>
                <description>Install obiee</description>
                <script>obiee/create-obiee</script>
                <order>60</order>
            </os-script>
        </steps>
    </rule>



</rules>